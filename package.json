{
  "name": "ngrx-store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lib-display:build": "ng build my-lib-display",
    "lib-display:build:watch": "ng build my-lib-display --watch",
    "server:mock": "mock-server --path ./mocks --proxy http://localhost:5000",
    "client:mock": "mock-client 3000 localhost 8083",
    "deploy": "npm run lib-display:build && ng build --prod ",
    "server:json": "json-server --watch db.json --port 5000  --delay 600",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.1",
    "@angular/cdk": "~13.2.1",
    "@angular/common": "~13.2.1",
    "@angular/compiler": "~13.2.1",
    "@angular/core": "~13.2.1",
    "@angular/forms": "~13.2.1",
    "@angular/material": "^13.2.1",
    "@angular/platform-browser": "~13.2.1",
    "@angular/platform-browser-dynamic": "13.2.1",
    "@angular/router": "~13.2.1",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/router-store": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "moment": "^2.29.1",
    "ng2-logger": "^8.0.20",
    "rxjs": "~7.4.0",
    "ts-to-zod": "^1.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.11.6",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.2.2",
    "@angular/cli": "~13.2.2",
    "@angular/compiler-cli": "~13.2.1",
    "@angular/language-service": "13.2.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@compodoc/compodoc": "^1.1.11",
    "@ngrx/schematics": "^13.0.2",
    "@schematics/angular": "13.2.2",
    "@storybook/addon-actions": "^6.5.0-alpha.34",
